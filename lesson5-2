#include <iostream>
using namespace std;
int main(){
	setlocale(LC_CTYPE, "Russian");
	int v, s;
	cout << " Высота : ";
	cin >> v;
	cout << "Ширина : ";
	cin >> s;
	for (int i = 0; i<s; i++)
	{
		for (int j = 0; j<v; j++)
		{
			if ((i >= 1 && i <= s - 2) && (j >= 1 && j <= v - 2))
				cout << " ";
			else
				cout << "*";
		}
		cout << endl;
	}
}
